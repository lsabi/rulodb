{
  "name": "@rulodb/rulodb",
  "description": "Typescript SDK for RuloDB",
  "version": "0.0.6",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Opcotech Developers",
    "email": "dev@opcotech.com"
  },
  "homepage": "https://github.com/rulodb/rulodb",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rulodb/rulodb"
  },
  "bugs": {
    "url": "https://github.com/rulodb/rulodb/issues"
  },
  "keywords": [
    "database",
    "rulodb",
    "rulo",
    "sdk"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run generate:proto",
    "prepare": "npm run build",
    "lint": "eslint src __tests__ --ext .ts",
    "test": "jest --config jest.config.js --verbose --no-cache",
    "watch": "rollup -c -w",
    "format": "prettier --write src __tests__",
    "generate:proto": "tsx scripts/generate-proto.ts"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.5.2",
    "generic-pool": "^3.9.0",
    "protobufjs": "^7.5.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.0.2",
    "prettier": "^3.5.3",
    "protobufjs-cli": "^1.1.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.0",
    "ts-jest": "^29.4.0",
    "ts-proto": "^2.7.5",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
